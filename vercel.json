{
    "version": 2,
    "builds": [
        {
            "src": "./target/src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "target/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./target/src/index.js"
        }
    ]
}